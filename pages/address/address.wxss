.item{
  padding-left: 20rpx;
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  color: #333;
  font-size: 30rpx;
  border-bottom:  1rpx solid #eee;
  margin-bottom: 30rpx;
}
input{
  padding-left: 20rpx;
}
.other{
  color: #333;
  font-size: 30rpx;
  padding-left: 20rpx;
  display: flex;
  align-items: center;
  height: 80rpx;
  border-bottom:  1rpx solid #eee;
}
.area{
  flex:4;
}
.choose{
  flex:1;
  color: #cdcdcd;
  font-size: 28rpx;
}
.choose image{
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
}
#commit{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 90%;
  height: 70rpx;
  border-radius: 50rpx;
  font-size: 30rpx;
  background-color: #66cc99;
}